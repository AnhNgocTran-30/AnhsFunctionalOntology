<http://fhnw.ch/functionalOntology#1.9.ManageEnterpriseKnowledgeManagement>
  rdf:type fo:Level-2-Sub-functions ;
  fo:IsSubFunctionOf <http://fhnw.ch/functionalOntology#1.0.StrategicManagement> ;
